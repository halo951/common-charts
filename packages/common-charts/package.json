{
    "name": "common-charts",
    "description": "适用于 vue2.x, 3.x 的 echart 图表工具, 内置了一些常用图表简化创建方法",
    "keywords": [
        "vue",
        "echart"
    ],
    "version": "0.0.1-dev.4",
    "main": "dist/index.cjs.js",
    "module": "dist/index.es.js",
    "typings": "typings/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.cjs.js"
        }
    },
    "files": [
        "package.json",
        "README.md",
        "tsconfig.json",
        "dist/"
    ],
    "sideEffects": false,
    "author": {
        "name": "halo951",
        "url": "https://github.com/halo951"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/halo951/common-charts.git"
    },
    "bugs": {
        "url": "https://github.com/halo951/common-charts/issues"
    },
    "homepage": "https://github.com/halo951/common-charts#readme",
    "license": "MIT",
    "scripts": {
        "clean": "rimraf dist/* && rimraf typings/*",
        "build": "rollup --config ./rollup.config.ts --configPlugin typescript2",
        "lint:fix": "prettier -w **.ts **.md **.json",
        "test": "jest --coverage"
    },
    "dependencies": {},
    "peerDependencies": {
        "echarts": ">= 5.0.0",
        "typescript": ">=4.4.4",
        "vue": "^2.6.14 || >= 2.7.0 || >= 3.0.0"
    },
    "devDependencies": {
        "@types/jest": "^28.1.8",
        "echarts": "^5.3.3",
        "jest": "^29.0.0",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.70.2",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-total-size": "^1.0.0",
        "rollup-plugin-typescript2": "^0.31.2",
        "ts-jest": "^28.0.8",
        "vue": "^3.2.37"
    }
}
